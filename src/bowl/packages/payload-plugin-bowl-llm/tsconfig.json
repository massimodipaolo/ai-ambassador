{
  "extends": "@websolutespa/tsconfig/tsreact.json",
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "coverage"
  ],
  "compilerOptions": {
    "outDir": "./dist",
    "module": "commonjs",
    "target": "es6",
    "sourceMap": true,
    "declarationDir": "./dist",
    "isolatedModules": false
  },
  "ts-node": {
    "transpileOnly": true
  }
}
