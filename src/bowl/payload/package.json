{
  "name": "@ia_ambassador/bowl",
  "version": "0.0.0",
  "private": true,
  "main": "dist/server.js",
  "license": "MIT",
  "scripts": {
    "dev:llm": "llm",
    "dev:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts nodemon",
    "dev": "run-s dev:*",
    "build:llm": "llm",
    "build:copy": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" \"./dist\"",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts NODE_ENV=production payload build",
    "build:server": "tsc",
    "build": "run-s build:*",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production ts-node dist/server.js",
    "start:serve": "npm run serve",
    "start": "run-s start:*",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
    "generate:graphQLSchema": "PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:graphQLSchema",
    "test:integration": "jest",
    "test": "run-s test:*",
    "test-watch": "npm test -- --watch",
    "test-e2e:install": "playwright install",
    "test-e2e:test": "playwright test",
    "test-e2e": "run-s test-e2e:*",
    "coverage:integration": "npm run test:integration -- --coverage",
    "coverage": "run-s -c --silent coverage:*"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.17.0",
    "@payloadcms/plugin-cloud-storage": "^1.0.12",
    "@payloadcms/plugin-seo": "^1.0.8",
    "@websolutespa/bom-core": "latest",
    "@websolutespa/bom-env": "latest",
    "@ia_ambassador/payload-plugin-bowl-llm": "*",
    "@websolutespa/payload-plugin-bowl": "latest",
    "@websolutespa/payload-plugin-cloud-storage-fs": "latest",
    "@websolutespa/payload-plugin-cron-job": "latest",
    "@websolutespa/payload-plugin-localization": "latest",
    "axios": "0.27.2",
    "express": "^4.17.1",
    "node-cron": "^3.0.3",
    "payload": "^1.11.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.29.1",
    "@swc/core": "1.3.100",
    "@swc/jest": "^0.2.24",
    "@swc/register": "0.1.10",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/express": "^4.17.9",
    "@types/jest": "^29.2.4",
    "@websolutespa/test": "latest",
    "@websolutespa/tsconfig": "latest",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint-config-websolute": "*",
    "eslint": "^8.46.0",
    "jest-environment-jsdom": "^29.3.1",
    "jest-fail-on-console": "^3.0.2",
    "jest-matcher-utils": "^29.3.1",
    "jest": "^29.3.1",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "overrides": {
    "graphql@>15.7.0 <16.7.0": "^15.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
